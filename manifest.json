{
	"name" : "OliviaWilde-ify!",
	"author" : "Randy Winch",
	"version" : "1.0",
	"manifest_version" : 2,
	"description" : "Add some flair to your web pages with the OliviaWilde-ify! Extension",
	"background": {
		"scripts": ["wildify.js"],
		"persistent": false
	},
	"icons" : {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"browser_action" : {
	    "default_icon": {
	      "19": "icons/icon19.png",
	      "38": "icons/icon38.png"
	    }
	},
	"permissions" : [
		"tabs",
		"http://*/*",
		"https://*/*"
	],
	"web_accessible_resources" : [
		"/loader.gif"
	]
}